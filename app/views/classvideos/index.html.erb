<% @classvideos.each do |c|%>
	<div class="containerblog">
	<li class="innercontainer" style="list-style-type: none; font-size: 20px;">
	<%= image_tag c.image.url(:medium) %>
		<br>
		<br>
		<%= c.post %>
		<br>
		<br>
		<% if user_signed_in? %>
		<%= link_to "Edit", edit_classvideo_path(c) %>
		<%= link_to "Delete", classvideo_path(c), method: :delete %>
		<% end %>
	</li>
	<p>
	<p>
</div>
<br>
<br>
<%end%>